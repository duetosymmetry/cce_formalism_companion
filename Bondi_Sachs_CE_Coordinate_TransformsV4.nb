Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"GenerateNewDummies", "[", "expr_", "]"}], "/;", 
    RowBox[{
     RowBox[{"Head", "@", "expr"}], "===", "Plus"}]}], ":=", 
   RowBox[{"GenerateNewDummies", "/@", "expr"}]}], ";"}]], "Input",
 CellLabel->"In[15]:=",ExpressionUUID->"ad4dff2b-b4e4-4073-adb5-92fe8173fbda"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"GenerateNewDummies", "[", "expr_", "]"}], "/;", 
    RowBox[{"!", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Head", "@", "expr"}], "===", "Plus"}], ")"}]}]}], ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"DummyList", ",", "ii", ",", "TempExpr"}], "}"}], ",", "\n", 
      "\t", 
      RowBox[{
       RowBox[{"DummyList", "=", 
        RowBox[{"UpIndex", "/@", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"IndicesOf", "[", "Dummy", "]"}], "[", "expr", "]"}], "/.", 
           RowBox[{"IndexList", "->", "List"}]}], ")"}]}]}], ";", "\n", "\t", 
       
       RowBox[{"TempExpr", "=", "expr"}], ";", "\n", "\t", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"ii", "=", "1"}], ",", 
         RowBox[{"ii", "<=", 
          RowBox[{"Length", "[", "DummyList", "]"}]}], ",", 
         RowBox[{"ii", "++"}], ",", "\n", "\t\t", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"BIndexQ", "[", 
             RowBox[{"DummyList", "[", 
              RowBox[{"[", "ii", "]"}], "]"}], "]"}], ",", "\n", "\t\t\t", 
            RowBox[{
             RowBox[{"With", "[", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{"ind", "=", 
                 RowBox[{"{", 
                  RowBox[{
                   RowBox[{"Unique", "[", 
                    RowBox[{
                    RowBox[{"DummyList", "[", 
                    RowBox[{"[", "ii", "]"}], "]"}], "[", 
                    RowBox[{"[", "1", "]"}], "]"}], "]"}], ",", 
                   RowBox[{
                    RowBox[{"DummyList", "[", 
                    RowBox[{"[", "ii", "]"}], "]"}], "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}], "}"}]}], "}"}], ",", 
               "\n", "\t\t\t\t", 
               RowBox[{"TempExpr", "=", 
                RowBox[{"TempExpr", "//.", 
                 RowBox[{"{", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"DummyList", "[", 
                    RowBox[{"[", "ii", "]"}], "]"}], ":>", "ind"}], ",", 
                   RowBox[{
                    RowBox[{"-", 
                    RowBox[{"DummyList", "[", 
                    RowBox[{"[", "ii", "]"}], "]"}]}], ":>", 
                    RowBox[{"-", "ind"}]}]}], "}"}]}]}]}], "]"}], ";"}], ",", 
            "\n", "\t\t\t", 
            RowBox[{
             RowBox[{"With", "[", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{"ind", "=", 
                 RowBox[{"Unique", "[", 
                  RowBox[{"DummyList", "[", 
                   RowBox[{"[", "ii", "]"}], "]"}], "]"}]}], "}"}], ",", "\n",
                "\t\t\t\t", 
               RowBox[{
                RowBox[{"TempExpr", "=", 
