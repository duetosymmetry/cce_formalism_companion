Cell["Rule generation", \
"Subsubsection",ExpressionUUID->"32681ae4-2aa4-456f-aebc-e207efc443dc"],

Cell["\<\
Unfortunately, the more general xTensor utilities that would do this sort of \
thing generate so many rules that the resulting notebook really drags, so we \
need to do a more specialized thing here.\
\>", "Text",ExpressionUUID->"64f5c7ef-9178-496f-9fd5-e8ba54f0e12d"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"IsSymmetric", "[", 
    RowBox[{"expr_", ",", "ind1_", ",", "ind2_"}], "]"}], ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", "swap", "}"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"expr", "//.", 
          RowBox[{"{", 
           RowBox[{"ind1", "\[Rule]", "swap"}], "}"}]}], "//.", 
         RowBox[{"{", 
          RowBox[{"ind2", "\[Rule]", "ind1"}], "}"}]}], "//.", 
        RowBox[{"{", 
         RowBox[{"swap", "\[Rule]", "ind2"}], "}"}]}]}], "]"}], "===", 
     "expr"}], ")"}]}], ";"}]], "Input",
 CellLabel->"In[17]:=",ExpressionUUID->"5c90e80f-b498-4036-8dab-88624923947e"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"CreateContractionAutoRules", "[", 
    RowBox[{"metric_", ",", "list_", ",", 
     RowBox[{"{", 
      RowBox[{"ind1_", ",", "ind2_"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"ind1Pattern_", ",", "ind2Pattern_"}], "}"}], ",", 
     RowBox[{"OptionsPattern", "[", "]"}]}], "]"}], ":=", "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"Evaluate", "@", 
           RowBox[{"Head", "[", 
            RowBox[{"#1", "[", 
             RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ")"}], "/:", 
         RowBox[{"Evaluate", "[", 
          RowBox[{
           RowBox[{"ContractMetric", "[", 
            RowBox[{
             RowBox[{"metric", "[", 
              RowBox[{
               RowBox[{"-", "ind1"}], ",", 
               RowBox[{"-", "ind2"}]}], "]"}], " ", "#1"}], "]"}], "/.", 
           "\[IndentingNewLine]", 
           RowBox[{"{", 
